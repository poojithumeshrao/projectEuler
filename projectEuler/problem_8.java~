public class problem_8{
    String inputString = "";
    //String[] inputString = new String[]{inputStringName};
    int[] inputArray= new int[1001];
     void problem_8(){
	 //int[] inputArray= new int[inputString.length()];
	
    }
    long multiply(int f){
	long prod;
	prod = inputArray[f]*inputArray[f+1]*inputArray[f+2]*inputArray[f+3];
	//System.out.println(prod);
	return prod;
    }
    int next(int t){
	
	int f=t;
	int i=4;
	//System.out.println(f);
	while(i>0){
	    if(f>=97){
		f=-1;
		return f;
	    }
	    f++;
	    if(inputArray[f]==0){
		i=4;
		t=f+1;
	    }
	}
	return t;
    }

    public static void main(String argv[]){
	//System.out.println(prob.inputString.length());
	problem_8 p8 = new problem_8();
	for(int i=0;i<1000;i++){
	    p8.inputArray[i]=p8.inputString.charAt(i) - '0';
	    //   System.out.println(p8.inputArray[i]);
	}
	int f =0;
	long high=0;
	long temp;
	    while(f!=-1){
	    temp =  p8.multiply(f);
	    if(temp>high)
		    high = temp;
		    f =p8.next(f);
	        System.out.println(f);
	    }
    }    
    
}
